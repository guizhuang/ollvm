bool CheckPatternPredicate(unsigned PredNo) const override {
  switch (PredNo) {
  default: llvm_unreachable("Invalid predicate in table?");
  case 0: return (PPCSubTarget->hasVSX());
  case 1: return (PPCSubTarget->hasAltivec());
  case 2: return (PPCSubTarget->hasQPX());
  case 3: return (!PPCSubTarget->hasOnlyMSYNC());
  case 4: return (PPCSubTarget->hasOnlyMSYNC());
  case 5: return (PPCSubTarget->hasHTM());
  case 6: return (PPCSubTarget->hasP8Vector());
  case 7: return (PPCSubTarget->hasICBT());
  case 8: return (PPCSubTarget->hasP8Altivec());
  case 9: return (PPCSubTarget->isPPC64());
  case 10: return (!PPCSubTarget->isPPC64());
  case 11: return (PPCSubTarget->hasExtDiv());
  case 12: return (PPCSubTarget->hasBPERMD());
  case 13: return (PPCSubTarget->hasP8Crypto());
  case 14: return (PPCSubTarget->hasDirectMove()) && (!PPCSubTarget->isLittleEndian());
  case 15: return (PPCSubTarget->hasDirectMove()) && (PPCSubTarget->isLittleEndian());
  case 16: return (!PPCSubTarget->isLittleEndian());
  case 17: return (PPCSubTarget->isLittleEndian());
  case 18: return (PPCSubTarget->hasP8Vector()) && (!PPCSubTarget->isLittleEndian());
  case 19: return (PPCSubTarget->hasP8Vector()) && (PPCSubTarget->isLittleEndian());
  case 20: return (PPCSubTarget->hasVSX()) && (!PPCSubTarget->isLittleEndian());
  case 21: return (PPCSubTarget->hasVSX()) && (PPCSubTarget->isLittleEndian());
  case 22: return (PPCSubTarget->isISA3_0());
  case 23: return (PPCSubTarget->hasDirectMove());
  case 24: return (PPCSubTarget->hasDirectMove()) && (PPCSubTarget->hasVSX());
  case 25: return (PPCSubTarget->hasP9Vector()) && (PPCSubTarget->isLittleEndian());
  case 26: return (PPCSubTarget->hasP9Vector()) && (!PPCSubTarget->isLittleEndian());
  case 27: return (PPCSubTarget->hasP9Vector());
  case 28: return (PPCSubTarget->hasQPX()) && (TM.Options.NoNaNsFPMath);
  case 29: return (PPCSubTarget->hasQPX()) && (!TM.Options.NoNaNsFPMath);
  }
