      10,	// subreg_hireg -> DoubleRegs_with_subreg_hireg_in_IntRegsLow8
      10,	// subreg_loreg -> DoubleRegs_with_subreg_hireg_in_IntRegsLow8
      0,	// subreg_overflow
    },
